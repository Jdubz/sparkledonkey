  "version": "0.0.1",
  "description": "Sparkle Donkey Village Website",
  "scripts": {
    "start": "gatsby develop",
    "buildStaging": "env GATSBY_ENV=staging gatsby build",
    "buildProd": "env GATSBY_ENV=production gatsby build",
