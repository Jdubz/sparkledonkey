    "gatsby-plugin-react-helmet": "^2.0.11",
    "gatsby-plugin-robots-txt": "^1.2.0",
    "gatsby-plugin-sass": "^1.0.26",
    "gatsby-plugin-sitemap": "^1.2.25",
    "normalize.css": "^8.0.0",
    "react": "^16.4.1",
    "react-click-outside": "^3.0.1",
    "react-dom": "^16.4.1",
